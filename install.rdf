<?xml version="1.0"?>
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">
	<Description about="urn:mozilla:install-manifest">
		<em:id>spamreport@expertspam.com</em:id>
		<em:version>1.60</em:version>
		<em:type>2</em:type>
		
		<!-- About the plugin -->
		<em:name>Expertspam</em:name>
		<em:description>Report spam, false-positives or phishing back to your antispam provider.</em:description>
		<em:creator>Your Antispam Company</em:creator>
		<em:developer>Your Antispam Company</em:developer>
		
		<!-- Preference window -->
		<em:optionsURL>chrome://expertspam/content/option.xul</em:optionsURL>
		
		<!-- Plugin icon -->
		<em:iconURL>chrome://expertspam/skin/icon.png</em:iconURL>
		
		<!-- Target Application this extension can install into, 
		with minimum and maximum supported versions. -->
		<em:targetApplication>
			<Description>
				<!-- Seamonkey -->
				<em:id>{92650c4d-4b8e-4d2a-b7eb-24ecf4f6b63a}</em:id>
				<em:minVersion>2.0.*</em:minVersion>
				<em:maxVersion>2.1.*</em:maxVersion>
			</Description>
			<Description>
				<!--Thunderbird-->
				<em:id>{3550f703-e582-4d05-9a08-453d09bdfdc6}</em:id>
				<em:minVersion>2.0</em:minVersion>
				<em:maxVersion>*</em:maxVersion>
			</Description>  
		</em:targetApplication>
	</Description>
</RDF>
